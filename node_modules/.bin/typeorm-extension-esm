#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/anjankc/Desktop/api/node_modules/.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules/typeorm-extension/bin/node_modules:/home/anjankc/Desktop/api/node_modules/.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules/typeorm-extension/node_modules:/home/anjankc/Desktop/api/node_modules/.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules:/home/anjankc/Desktop/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/anjankc/Desktop/api/node_modules/.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules/typeorm-extension/bin/node_modules:/home/anjankc/Desktop/api/node_modules/.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules/typeorm-extension/node_modules:/home/anjankc/Desktop/api/node_modules/.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules:/home/anjankc/Desktop/api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules/typeorm-extension/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/typeorm-extension@3.6.3_typeorm@0.3.20_pg@8.13.3_ts-node@10.9.2_@types+node@20.17.19_typescript@5.7.3__/node_modules/typeorm-extension/bin/cli.mjs" "$@"
fi
